<input
  type='password'
  name='<%= @name %>'
  class='<%= @class %>
         ui-corner-all
         form-add-input
         dative-tooltip
         dative-input-field
         input-only'
  <% for attribute, value of @domAttributes: %>
    <%= attribute %>="<%= value %>"
  <% end %>
  title='<%= @title %>'
  value='<%= @value %>'>

